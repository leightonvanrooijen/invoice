{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:import/recommended",
        "standard",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "unused-imports"
    ],
    "rules": {
        "quotes": ["error", "double"],
        "unused-imports/no-unused-imports": "error",
        "no-unused-vars": "warn",
        "no-useless-constructor": "off",
        "import/no-unresolved": "off",
        "import/no-cycle": "error"
    }
}
